    <script>
        var chart;
$(document).ready(function() {

    var colors = Highcharts.getOptions().colors,
        categories = ['商品一', '商品二', '商品三', '商品四', '商品五'],
        name = '',
        data = [{
                y: 55.11,
                color: colors[0]
            }, {
                y: 21.63,
                color: colors[1]
            }, {
                y: 11.94,
                color: colors[2]
            }, {
                y: 7.15,
                color: colors[3]
            }, {
                y: 2.14,
                color: colors[4]
            }];

    function setChart(name, categories, data, color) {
        chart.xAxis[0].setCategories(categories);
        chart.series[0].remove();
        chart.addSeries({
            name: name,
            data: data,
            color: color || 'white'
        });
    }

    chart = new Highcharts.Chart({
        chart: {
            renderTo: 'container',
            type: 'column'
        },
        title: {
            text: '导购专家为您推荐'
        },
        subtitle: {
            text: ''
        },
        xAxis: {
            categories: categories
        },
        yAxis: {
            title: {
                text: ''
            }
        },
        plotOptions: {
            column: {
                cursor: 'pointer',
                point: {
                    events: {
                        click: function() {
                            $('#test').click();
                        }
                    }
                },
                dataLabels: {
                    enabled: true,
                    color: colors[0],
                    style: {
                        fontWeight: 'bold'
                    },
                    formatter: function() {
                        return this.y +'%';
                    }
                }
            }
        },
        tooltip: {
            formatter: function() {
                var point = this.point,
                    s = this.x +':<b>'+ this.y +'%</b>';
                return s;
            }
        },
        series: [{
            name: name,
            data: data,
            color: 'white'
        }],
        exporting: {
            enabled: false
        }
    });
});
    </script>

    
    

